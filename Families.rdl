  <DataSets>
    <DataSet Name="DS">
      <Query>
        <DataSourceName>ChurchDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@GroupId">
            <Value>=Parameters!GroupId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>with cteFamilies as (
    select distinct
        gr.GroupId,
        gr.GroupName,
        f.FamilyId,
        f.CustomerNumber as PrimaryCustomerNumber,
		cust.LastName as PrimaryLastName,
        a.Street + ', ' + a.City + ', ' + a.State + ' ' + a.PostalCode as Address,
        Phone = (SELECT TOP 1 p.PhoneNumber
                             FROM dbo.Phone p
                             WHERE p.PhoneBookId = cust.PhoneBookId
                             AND p.PhoneNumber IS NOT NULL AND
                             LEN(p.PhoneNumber) &gt; 0
                             )
    from FamilyMember f
        JOIN Customer cust ON cust.CustomerNumber = f.CustomerNumber and cust.StatusId =1
        JOIN PolicyItem PI ON cust.CustomerId = PI.CustomerId and pi.StatusId = 1
        JOIN Address A on cust.AddressId = A.AddressId
        join PolicyHeader ph on pi.PolicyId = ph.PolicyId
        join [Group] gr on gr.GroupId = ph.PolicyGroupId
    where 1=1
    --   and f.FamilyId = 131
      and f.RelationshipId = 7
--       and f.FamilyId in (select distinct FamilyId from FamilyMember fm2)
    )


select
      cteFm.GroupId
     , cteFm.GroupName
     , fm.FamilyId
     , fm.CustomerNumber
     , cust.LastName + ' ' + cust.FirstName as Name
     , cust.BirthDate
     , r2.RelationshipName
     , cteFm.phone
     , cteFm.Address


from FamilyMember fm
join Customer cust ON cust.CustomerNumber = fm.CustomerNumber and cust.StatusId =1
join cteFamilies cteFm on cteFm.FamilyId = fm.FamilyId
left join Relationship R2 on fm.RelationshipId = R2.RelationshipId
where 1 = 1
and cteFm.GroupId = @GroupId
order by GroupName,
         PrimaryLastName,
         iif(r2.RelationshipId =7, 0, 99),
         cust.BirthDate</CommandText>
      </Query>
