  <DataSets>
    <DataSet Name="SummaryDS">
      <Query>
        <DataSourceName>ChurchDS</DataSourceName>
        <CommandText>SELECT  cyl.CycleId
	   ,cyl.CycleName
       ,cyl.StartDate
       ,cyl.EndDate
       ,Credit = CAST(SUM(CASE WHEN e.IsDebit=0 THEN e.ExpenseAmount ELSE 0 END) AS DECIMAL(18,0)) +
				 (SELECT SUM(i.AmountPaid) FROM dbo.Invoice i WHERE i.CycleId = cyl.CycleId)
       ,Debit = CAST(SUM(CASE WHEN e.IsDebit=1 THEN e.ExpenseAmount ELSE 0 END) AS DECIMAL(18,0))
      
FROM dbo.Expense e
JOIN dbo.Cycle cyl ON e.ExpenseDate BETWEEN cyl.StartDate AND cyl.EndDate
GROUP BY cyl.CycleId,
		 cyl.CycleName,
		 cyl.StartDate,
         cyl.EndDate
ORDER BY cyl.StartDate desc</CommandText>
      </Query>
      <Fields>
