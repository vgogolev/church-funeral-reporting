  <DataSets>
    <DataSet Name="FuneralClaimsDS">
      <Query>
        <DataSourceName>ChurchDS</DataSourceName>
        <CommandText>SELECT 
	   ID = e.ExpenseId
      
      ,ExpenseDate = CAST(e.ExpenseDate AS DATE)
      ,[Account] = a.AccountName
      ,[Group] = ISNULL(g.GroupName, 'Undefined')
      ,Debit = CASE WHEN IsDebit = 1 THEN e.ExpenseAmount ELSE 0 END
      ,Credit = CASE WHEN IsDebit = 0 THEN e.ExpenseAmount ELSE 0 END
      ,Description = e.ExpenseDescription
      
      
      ,e.IsDebit
      ,e.GroupId
      ,e.AccountId
  FROM Church.dbo.Expense e
  LEFT OUTER JOIN dbo.[Group] g ON e.GroupId = g.GroupId
  LEFT OUTER JOIN dbo.Account a ON e.AccountId = a.AccountId
  WHERE e.AccountId = 6
  ORDER BY e.ExpenseDate DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="ID">
          <DataField>ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ExpenseDate">
          <DataField>ExpenseDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Account">
