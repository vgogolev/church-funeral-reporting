  <DataSets>
    <DataSet Name="ContributionByChurchDS">
      <Query>
        <DataSourceName>ChurchDS</DataSourceName>
        <CommandText>SELECT  
        i.CycleId
       ,cl.CycleName
       
       ,[Group] = ISNULL(g.GroupName,'Undefined')
       ,cl.StartDate
       ,Credit = SUM(i.AmountPaid)
FROM dbo.Invoice i
LEFT OUTER JOIN dbo.Cycle cl ON cl.CycleId = i.CycleId
LEFT OUTER JOIN dbo.PolicyHeader ph ON ph.PolicyNumber = i.PolicyNumber
LEFT OUTER JOIN dbo.[Group] g ON g.GroupId = ph.PolicyGroupId
WHERE AccountId = 1
GROUP BY  i.CycleId
       ,cl.StartDate
       ,cl.CycleName
       ,g.GroupName
ORDER BY cl.StartDate desc</CommandText>
      </Query>
      <Fields>
        <Field Name="CycleId">
          <DataField>CycleId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="CycleName">
          <DataField>CycleName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Group">
          <DataField>Group</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="StartDate">
          <DataField>StartDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Credit">
          <DataField>Credit</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
