    <DataSet Name="GroupsDS">
      <Query>
        <DataSourceName>ChurchDS</DataSourceName>
        <CommandText>SELECT  g.GroupId
       ,g.GroupName
FROM    dbo.[Group] g
WHERE   EXISTS ( SELECT NULL
                 FROM   dbo.PolicyHeader ph
                 WHERE  ph.PolicyGroupId = g.GroupId )
ORDER BY g.GroupName</CommandText>
      </Query>
      <Fields>
        <Field Name="GroupId">
          <DataField>GroupId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="GroupName">
          <DataField>GroupName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
