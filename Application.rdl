  <DataSets>
    <DataSet Name="CurrentInvoicesDS">
      <Query>
        <DataSourceName>ChurchDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@GroupId">
            <Value>=Parameters!GroupId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@InvoiceId">
            <Value>=Parameters!InvoiceId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ExcludeConverted">
            <Value>=Parameters!ExcludeConverted.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @CycleId INT

SELECT @CycleId = MAX(CycleId) FROM dbo.Cycle cl WHERE GETDATE() BETWEEN StartDate AND EndDate

SELECT
       [i].[InvoiceId]
      ,IIF(cnvtd.PolicyNumber is null, 0, 1) OnlineConverted
      ,i.CycleId
      ,cl.CycleName
      ,i.CustomerId
      ,[FullName] = c.[LastName] + ', ' + c.[FirstName]
      ,[i].[AmountDue]
      ,addr.Street
      ,addr.City
      ,addr.State
      ,addr.PostalCode
      ,Phone = (SELECT p1.PhoneNumber FROM dbo.Phone p1 WHERE p1.PhoneBookId = c.PhoneBookId AND p1.PhoneTypeId = 1)
      ,PhoneAlt = (SELECT p1.PhoneNumber FROM dbo.Phone p1 WHERE p1.PhoneBookId = c.PhoneBookId AND p1.PhoneTypeId = 2)
  FROM [dbo].[Invoice] i
  JOIN dbo.Cycle cl ON cl.CycleId = i.CycleId
  JOIN dbo.PolicyHeader ph ON i.PolicyNumber = ph.PolicyNumber AND ph.PolicyGroupId = @GroupId
  JOIN dbo.Customer c ON c.CustomerId = i.CustomerId
  LEFT JOIN dbo.Address addr ON addr.AddressId = c.AddressId
  LEFT JOIN dbo.mfc_converted_vw cnvtd on i.PolicyNumber = cnvtd.PolicyNumber
  WHERE i.CycleId = @CycleId
  and i.InvoiceId = case when (@InvoiceId is null or @InvoiceId = '') then InvoiceId else @InvoiceId end
  and IIF(cnvtd.PolicyNumber is null, 1, 0) =case when (@ExcludeConverted = 0) then IIF(cnvtd.PolicyNumber is null, 1, 0) else @ExcludeConverted end
  ORDER BY [FullName]</CommandText>
      </Query>
